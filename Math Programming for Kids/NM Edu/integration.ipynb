    "// the three-leg speed function\n",
    "fun speed(t : Double): Double { // t = time\n",
    "    if (t <= 5.0) {\n",
    "        return 1.0;\n",
    "    }\n",
    "    else if (t <= 10.0) { // t > 5.0\n",
    "        return 5.0;\n",
    "    }\n",
    "    else { // t > 10.0\n",
    "        return 3.0;\n",
    "    }\n",
    "}"
   ]
  }
