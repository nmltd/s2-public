   "source": [
    "/**\n",
    " * Compute the determinant of a matrix.\n",
    " *\n",
    " * @author Haksun Li\n",
    " */\n",
    "\n",
    "%use s2\n",
    "\n",
    "// Create a matrix\n",
    "val A = DenseMatrix(arrayOf(\n",
    "    doubleArrayOf(3.0, 5.0), \n",
    "    doubleArrayOf(7.0, 4.0)))\n",
    "// Compute the determinant\n",
    "val det: Double = MatrixMeasure.det(A)\n",
    "println(\"determinant: $det\")"
   ]
